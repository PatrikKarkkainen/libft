#include "libft.h"
#include <stdio.h>

int	main()
{
	char	*ptr;
	char	**arr;

	arr = (char**)malloc(sizeof(char*));
	ptr = ft_memalloc(sizeof(char) * 3);
	arr[0] = ptr;
	ptr = "42";

	printf("%s\n", ptr);
	ft_strdel(arr);
	printf("%s\n", ptr);
	return (0);
}
